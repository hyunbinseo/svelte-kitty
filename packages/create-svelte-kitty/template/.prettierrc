{
	"useTabs": true,
	"singleQuote": true,
	"quoteProps": "consistent",
	"trailingComma": "all",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte", "prettier-plugin-tailwindcss"],
	"overrides": [
		{ "files": "*.md", "options": { "useTabs": false, "tabWidth": 2 } },
		{ "files": "*.svelte", "options": { "parser": "svelte" } }
	]
}
